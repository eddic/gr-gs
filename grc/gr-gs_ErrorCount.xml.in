<?xml version="1.0"?>
<block>
    <name>Error Count</name>
    <key>ErrorCount</key>
    <category>Guided Scrambling</category>
    <import>import gr_gs as gs</import>
    <make>gs.ErrorCount($framingTag)</make>
    <param>
        <name>Framing Tag</name>
        <key>framingTag</key>
        <value></value>
        <type>string</type>
        <hide>part</hide>
    </param>
    <sink>
        <name>fast</name>
        <type>@PYTHON_SYMBOL_TYPE@</type>
    </sink>
    <sink>
        <name>slow</name>
        <type>@PYTHON_SYMBOL_TYPE@</type>
    </sink>
    <source>
        <name>out</name>
        <type>f32</type>
    </source>
    <doc>This block is used for calculating total error counts and rates for framed symbol sequences over all time.</doc>
</block>
